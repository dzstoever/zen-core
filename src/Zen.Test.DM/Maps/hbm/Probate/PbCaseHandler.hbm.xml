<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Zen.Test.DM" 
                   namespace="Zen.Test.Domain.Entities.Probate" >

  <class name="PbCaseHandler" table="probe.dbo.[probe_cs handler 1]">    
    
    <id name="Id" column="[cs handler 1]">
      <generator class="assigned" />
    </id>
    
    <property name="Description" column="[full name]" />
    <property name="Name" formula="[cs fname] + ' ' + [cs lname]" />      
    
  </class>

</hibernate-mapping>