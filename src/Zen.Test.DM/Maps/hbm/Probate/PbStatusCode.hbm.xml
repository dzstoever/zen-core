    <property name="IsAuto" />
    <property name="IsChristus" />
    <property name="IsClosable" />
    <property name="IsEL2" />
    <property name="IsHearingType" />
    <property name="IsLegal" />
    <property name="IsNA" />
    <property name="IsPTP" />
    <property name="IsPost" />
    <property name="IsPre" />
    <property name="IsRetired" />
    <property name="IsScorable" />
    <property name="IsStatus1" />
    <property name="IsStatusA" />
    <property name="IsUncollectible" column="[IsRptUncollectible]" />        
    
  </class>

</hibernate-mapping>