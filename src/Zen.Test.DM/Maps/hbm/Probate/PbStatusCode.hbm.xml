                   namespace="Zen.Test.Domain.Entities.Probate">

  <class name="PbStatusCode" table="probe.dbo.[p2_lu_statuscode]">    
    
    <id name="Id" column="[statuscode]">
      <generator class="assigned" />
    </id>
    
    <property name="IsActiveInventory" />
