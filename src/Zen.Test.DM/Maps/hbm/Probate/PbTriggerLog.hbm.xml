<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Zen.Test.DM"  
                   namespace="Zen.Test.Domain.Entities.Probate">

  <class name="PbTriggerLog" table="probe.dbo.[p2_triggerlog]">
    
    <id name="Id" column="[id]">
      <generator class="native" />
    </id>    
    
    <property name="CreateDate" />
    <property name="FieldName" />
    <property name="NewVal" />
    <property name="OldVal" />
    <property name="ProcessName" />
    <property name="ServerName" />
    <property name="TableName" />
    <property name="UserId" />        
    <property name="Workstation" />
    <property name="DatabaseName" />                
    <property name="Key1Name" />
    <property name="Key1Val" />
    <property name="Key2Name" />
    <property name="Key2Val" />
    <property name="Key3Name" />
    <property name="Key3Val" />
    
    <!-- one-way association 
    <many-to-one name="Account" 
                 class="IPbAccount" column="[key1val]" />-->
    
  </class>

</hibernate-mapping>