<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="Zen.Test.DM"  namespace="Zen.Test.Domain.Entities.Collections">
  
  <class name="BkWorkMaintenance" table="bkcol.dbo.[wrk_mnt]" 
         dynamic-insert="true" dynamic-update="true">
    
    <id name="Id" column="[idx]">
      <generator class="native" />
    </id>
    
    <property name="IsOnFile" />
    <property name="CommissionRate" />
    <property name="GrossAmount" column="[grossamt]"/>
    <property name="NetAmount" column="[netamt]"/>
    <property name="AcctId" />
    <property name="AcctNum" />
    <property name="ClientMaster" column="[clientmastershortname]" />
    <property name="FieldCode" />
    <property name="InputFile" />
    <property name="MaintData" column="[mntdata]" />
    <property name="RMSCode" />
    <property name="RecordType" />
    <property name="TranCode" />    
    <property name="UD01" />
    <property name="UD02" />
    <property name="UD03" />
    <property name="UD04" />
    <property name="UD05" />
    <property name="UD06" />
    <property name="UD07" />
    <property name="UD08" />
    <property name="UD09" />
    <property name="UD10" />
    <property name="CreateDate" />
    <property name="TranDate" />
    <property name="DebtorNum" />
    <property name="RefNum" />
    <property name="ClientShort" column="[clientshortname]" />
    <property name="ErrorMessage" />
    <property name="RecordImage" />
    <property name="StatusCode" />
    
    <many-to-one name="Batch"
                 class="BkWorkBatch" column="[batchid]" />

  </class>

</hibernate-mapping>