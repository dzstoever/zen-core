<?xml version="1.0"?>
<configuration>
  <!---->
  <configSections>
    <section name="hibernate-configuration" type="NHibernate.Cfg.ConfigurationSectionHandler, NHibernate" />    
  </configSections>
    
  <!---->
  <connectionStrings>
    <add name="TestDbCnn" connectionString="Server=.\SQLExpress; Initial Catalog=ZenTestDb; Integrated Security=true;" />
  </connectionStrings>
    
  <!--
  <appSettings>
  </appSettings>-->
    
  <!---->
  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <session-factory name="AppConfig.SessionFactory">
      <property name="connection.connection_string_name">TestDbCnn</property>
      <property name="dialect">NHibernate.Dialect.MsSql2008Dialect</property>      
      <mapping assembly="Zen.Test" />      
    </session-factory>
  </hibernate-configuration>
  
</configuration>
